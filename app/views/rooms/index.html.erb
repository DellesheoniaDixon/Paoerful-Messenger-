<div class="row">
    <div class="col-md-2">
        <h5> Welcome <%= @current_user.username %> </h5>
        <h4> Users </h4>
        <%= render @users%>
        <h4> Rooms </h4>
        <%= render partial: "layouts/new_room_form" %>
        <%= render @rooms %>
    </div>
    <div class="col-md-10 bg-dark">
        <% if @single_room %>
            <h4 class="text-center"> <%= @single_room.name %> </h4>
        <% end %>
    </div>
</div>